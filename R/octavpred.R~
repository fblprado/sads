octavpred <- function(x=NULL,rich=NULL,sad,oct=NULL,...){
  if(is.null(rich)) S <- length(x)
  if(is.null(oct)&!is.null(x))oct <- 1:(ceiling(max(log2((x))))+1)
  n <- 2^(oct-1)
  psad <- paste("p",sad,sep="")
  dots <- list(...)
  Y <- do.call(psad,c(list(q=n),dots))
  Y <- c(Y[1],diff(Y))*S
  new("octav",data.frame(octave=oct, upper=factor(n), Freq=Y))
}
